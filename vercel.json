{
    "public": true,
    "routes": [
        {
            "src": "/",
            "dest": "/"
        },
        {
            "src": "/coming-soon",
            "dest": "/"
        },
        {
            "src": "/history/humanos",
            "dest": "/"
        },
        {
            "src": "/history/venyator",
            "dest": "/"
        },
        {
            "src": "/history/gigerianus",
            "dest": "/"
        },
        {
            "src": "/assets/(.*)",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        }
    ],
    "headers": [
        {
            "source": "/assets/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        }
    ]
}